{
  "name": "tutorhomie",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "db:migrate:up": "knex migrate:latest --knexfile ./src/db/knexfile.js ",
    "db:migrate:down": "knex migrate:rollback --knexfile ./src/db/knexfile.js ",
    "db:migrate:make": "knex migrate:make --knexfile ./src/db/knexfile.js ",
    "db:seed": "knex seed:run --knexfile ./src/db/knexfile.js",
    "start:dev": "nodemon --exec npm run babel-node -- ./src/server/server.js",
    "webdev:start": "webpack-dev-server",
    "test": "npm run babel-node index.js"
  },
  "author": "Ian Deans",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "knex": "^0.13.0",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
